{
  "name": "theme-vitesse-grey",
  "displayName": "Vitesse Theme Gray",
  "version": "0.1.14",
  "description": "Vitesse grey theme for VS Code",
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "vitesse",
    "light",
    "dark"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/loosheng/vscode-theme-vitesse"
  },
  "files": [
    "themes"
  ],
  "bugs": {
    "url": "https://github.com/loosheng/vscode-theme-vitesse/issues"
  },
  "publisher": "loosheng",
  "scripts": {
    "build": "esno src/index.ts",
    "dev": "nodemon --watch src -e ts --exec \"esno src/index.ts\"",
    "release": "yarn build && npx bumpp --commit --tag --push --all",
    "publish": "vsce publish --no-dependencies --allow-star-activation",
    "pack": "vsce package --no-dependencies --allow-star-activation"
  },
  "contributes": {
    "themes": [
      {
        "label": "Vitesse Gray Light",
        "uiTheme": "vs",
        "path": "./themes/vitesse-light.json"
      },
      {
        "label": "Vitesse Gray Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/vitesse-dark.json"
      }
    ]
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.6.6",
    "@antfu/utils": "^0.0.3",
    "@types/color": "^3.0.3",
    "@types/node": "^14.18.12",
    "color": "^3.2.1",
    "eslint": "^7.32.0",
    "esno": "^0.5.0",
    "nodemon": "^2.0.15",
    "tsup": "^4.14.0",
    "typescript": "^4.6.3"
  },
  "engines": {
    "vscode": "^1.43.0"
  },
  "icon": "icon.png",
  "preview": true
}
